@model Rolepp.Models.Warehouse

@{
    ViewData["Title"] = "Create Warehouse";
}

<h2>Create Warehouse</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="WarehouseName" class="control-label"></label>
        <input asp-for="WarehouseName" class="form-control" />
        <span asp-validation-for="WarehouseName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Address" class="control-label"></label>
        <input asp-for="Address" class="form-control" />
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
