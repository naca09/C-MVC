@model IEnumerable<Rolepp.Models.Warehouse>

@{
    ViewData["Title"] = "Warehouse List";
}

<h2>Warehouse List</h2>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.WarehouseName)</th>
            <th>@Html.DisplayNameFor(model => model.Address)</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var warehouse in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => warehouse.WarehouseName)</td>
                <td>@Html.DisplayFor(modelItem => warehouse.Address)</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@warehouse.WarehouseID">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@warehouse.WarehouseID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Create">Create New Warehouse</a>
